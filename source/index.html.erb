---
title: Welcome to Middleman
---

<div class="main">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-6 col-md-offset-3">
        <h3 id="collection-title-form">List</h3>
        <form action="/" class="one-page-form collection-form">
          <div class="form-group">
            <!-- <label for="">title</label> -->
            <input type="text" class="form-control collection-input" id="collection-title-input" placeholder="Title">
          </div>
          <div class="form-group">
            <!-- <label for="">tagline</label> -->
            <input type="text" class="form-control collection-input" id="collection-tagline-input" placeholder="Tagline">
          </div>
          <div class="form-group">
            <textarea type="text" class="form-control collection-input" id="collection-description-input" placeholder="Description"></textarea>
          </div>
        </form>
        <h4 id="resource-title-form">Resources</h3>
        <form action="/" class="one-page-form resource-form">
          <div class="form-group">
            <!-- <label for="">title</label> -->
            <input type="text" class="form-control resource-input resource-title" placeholder="Title">
          </div>
          <div class="form-group">
            <!-- <label for="">description</label> -->
            <input type="url" class="form-control resource-input resource-url" placeholder="Url">
          </div>
          <div class="form-group">
            <!-- <label for="">tagline</label> -->
            <textarea type="text" class="form-control resource-input resource-content" placeholder="Content"></textarea>
          </div>
        </form>
        <button>Add</button>
      </div>
    </div>
    <button>Publish</button>
  </div>
</div>
